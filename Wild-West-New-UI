local Luminosity = loadstring(game:HttpGet("https://raw.githubusercontent.com/iHavoc101/Genesis-Studios/main/UserInterface/Luminosity.lua", true))()

local Window = Luminosity.new("BlankHack", "By Sora_Blank", 4370345701)

local Tab1 = Window.Tab("Esp", 6026568198)
local ESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/1201for/V.G-Hub/main/Karrot-Esp"))()
local PlayerEsp = Tab1.Folder("Player", "Возможно получить Бан", function(Status)
PlayerEsp .Switch("Player Esp", function(Status)
ESP:Toggle(State)
end)
PlayerEsp .Switch("Tracers Esp", function(Status)
ESP:Toggle(State)
end)
PlayerEsp .Switch("Player Esp", function(Status)
ESP:Toggle(State)
end)
PlayerEsp .Switch("Player Esp", function(Status)
ESP:Toggle(Status)
end)

-- Tab 2 --
local Tab2 = Window.Tab("Misc", 6022668945)
Tab2.Folder("About", "Добро пожаловать в чит BlankHack, этот чит будет продвигаться и дальше, надеюсь вы поддержите меня :3")
local Cheat = Tab2.Folder("Game", "A bunch of options you can use", function(Status)
Cheat.Button("Rejoin", "Перезайти", function()
game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer) end)

game:GetService("UserInputService").InputBegan:Connect(function(Input)
if Input.KeyCode == Enum.KeyCode.X then
Window:Toggle()
end
end)
